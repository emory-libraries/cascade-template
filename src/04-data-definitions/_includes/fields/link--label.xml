---
id: link-label
label: Link Label
message: Enter a custom label for this link.
---

{{#use 'link--label' with=(array 'customizations' 'force')}}
{{#if (or (neq customizations.label false) (includes 'label' force))}}
<text help-text="{{message}}"
      identifier="{{id}}"
      label="{{label}}"
      required="true"/>
{{/if}}
{{/use}}
