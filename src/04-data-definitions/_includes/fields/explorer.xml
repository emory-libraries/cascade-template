---
id: links-list
label: Explorer
---

{{#use 'explorer' with='nesting'}}
<group identifier="{{id}}" label="{{label}}">
  {{> heading--label max=15 required=true default='Popular Links'}}
  {{> link multiple=true
           min=3
           max=8
           options=(object page=true file=true url=true)
           customizations=(object label=true description=false)
           nesting=(breadcrumb id)
           required='label'}}
</group>
{{/use}}
